    "prettier": "^2.2.1",
    "stylelint": "^13.8.0",
    "stylelint-order": "^4.1.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "eslintIgnore": ["node_modules", "coverage", "dist", "build", "docs", ".vscode", ".github", ".idea", ".storybook"],
  "lint-staged": {
    "{,!(node_modules)/**/}*.{ts,tsx,js}": ["eslint --cache --fix", "prettier --write"],
    "{,!(node_modules)/**/}*.{css,scss}": ["prettier --write", "stylelint --cache --fix"]
  },
  "jest": {
    "transformIgnorePatterns": ["node_modules/(?!react-syntax-highlighter)/"]
  },
  "keywords": [],
  "description": ""
