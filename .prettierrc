{
  "bracketSpacing": false,
  "endOfLine": "auto",
  "printWidth": 120,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "tabWidth": 2,
  "useTabs": false,
  "proseWrap": "preserve",
  "jsxSingleQuote": false,
  "jsxBracketSameLine": false,
  "quoteProps": "as-needed",
  "htmlWhitespaceSensitivity": "css",
  "arrowParens": "always"
}